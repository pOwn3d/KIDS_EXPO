{
  "name": "kids-points-app",
  "version": "1.0.0",
  "homepage": "https://pown3d.github.io/KIDS_EXPO",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web --output-dir dist",
    "deploy": "gh-pages -d dist",
    "predeploy": "npm run build:web",
    "eject": "expo eject",
    "test": "jest",
    "test:e2e": "detox test",
    "test:e2e:ios": "detox test --configuration=ios.sim.release",
    "test:e2e:android": "detox test --configuration=android.emu.release",
    "build:e2e:ios": "detox build --configuration=ios.sim.release",
    "build:e2e:android": "detox build --configuration=android.emu.release",
    "detox:build:ios": "detox build --configuration=ios.sim.release",
    "detox:build:android": "detox build --configuration=android.emu.release",
    "detox:test:ios": "detox test --configuration=ios.sim.release --cleanup",
    "detox:test:android": "detox test --configuration=android.emu.release --cleanup",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
